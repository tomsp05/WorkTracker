{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Build WorkTracker",
			"type": "shell",
			"command": "xcodebuild",
			"args": [
				"-project",
				"WorkTracker.xcodeproj",
				"-scheme",
				"WorkTracker",
				"-configuration",
				"Debug",
				"-destination",
				"generic/platform=iOS Simulator",
				"build"
			],
			"group": "build",
			"isBackground": false,
			"problemMatcher": [
				"$gcc"
			]
		}
	]
}